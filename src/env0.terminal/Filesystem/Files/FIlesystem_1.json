{
  "filesystem": {
    "name": "Basic UNIX",
    "root": {
      "home": {
        "user": {
          "welcome.txt": {
            "type": "text",
            "content": "Welcome to your basic UNIX machine!",
            "size": 45,
            "permissions": "rw-r--r--"
          },
          "about.bin": {
            "type": "binary",
            "content": "binarydatahere",
            "size": 1024,
            "permissions": "r--r--r--"
          }
        }
      },
      "etc": {
        "hostname.txt": {
          "type": "text",
          "content": "UNIX Server",
          "size": 13,
          "permissions": "rw-r--r--"
        }
      },
      "var": {
        "log": {
          "system.log": {
            "type": "text",
            "content": "System log data...",
            "size": 150,
            "permissions": "rw-r--r--"
          }
        }
      }
    }
  }
}
